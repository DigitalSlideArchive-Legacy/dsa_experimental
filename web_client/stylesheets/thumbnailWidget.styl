.dsa-item-grid
  padding 0
  margin 0
  .g-show-more
    display block
.dsa-item-grid-entry
  display inline-block
  width 128px
  height 128px
  vertical-align middle
  position relative
  a
    border none
    height 100%
    width 100%
    img
      border none
      margin auto
      position absolute
      top 0
      bottom 0
      left 0
      right 0
      max-width 100%
      max-height 100%
